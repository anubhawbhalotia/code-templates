<snippet>
	<content><![CDATA[
template <typename T>
int bitwiseGetCountSetBit(T a)
{
	int c = 0;
	while(a)
	{
		if(a % 2 == 1)
			c++;
		a /= 2;
	}
	return c;
}
]]></content>
	<!-- Optional: Set a tabTrigger to define how to trigger the snippet -->
	<tabTrigger>bitwiseGetCountSetBit</tabTrigger>
