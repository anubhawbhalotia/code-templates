vi getDivisors(int n)
{
	vi ans;
	fe(i, 1, (int)sqrt(n))
	{
		if(n % i == 0)
		{
			ans.pb(i);
			if(i * i != n)
				ans.pb(n / i);
		}
	}
	return ans;
}
]]></content>
	<!-- Optional: Set a tabTrigger to define how to trigger the snippet -->
	<tabTrigger>getDivisors</tabTrigger>
